        <property name="oriInvoiceType" type="java.lang.String">
            <column name="ori_invoice_type" />
        </property>
        <property name="oriInvoiceAmount" type="java.lang.Double">
            <column name="ori_invoice_amount" precision="9" />
        </property>
        <property name="oriObjectId" type="java.lang.String">
            <column name="ori_object_id" />
        </property>
        <property name="oriObjectNo" type="java.lang.String">
            <column name="ori_object_no" />
        </property>
        <property name="oriObjectName" type="java.lang.String">
            <column name="ori_object_name" />
