        </property>
        <property name="invoiceType" type="java.lang.String">
            <column name="invoice_type" />
        </property>
        <property name="invoiceAmount" type="java.lang.Double">
            <column name="invoice_amount" />
        </property>
        <property name="objectId" type="java.lang.String">
            <column name="object_id" />
        </property>
        <property name="objectNo" type="java.lang.String">
            <column name="object_no" />
        </property>
        <property name="objectName" type="java.lang.String">
