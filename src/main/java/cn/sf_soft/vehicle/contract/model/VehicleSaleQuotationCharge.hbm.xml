<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.sf_soft.vehicle.contract.model.VehicleSaleQuotationCharge" table="vehicle_sale_quotation_charge"
           schema="dbo">
        <id name="selfId" column="self_id"/>
        <property name="chargeId" column="charge_id"/>
        <property name="chargeName" column="charge_name"/>
        <property name="income" column="income"/>
        <property name="chargePf" column="charge_pf"/>
        <property name="chargeComment" column="charge_comment"/>
        <property name="remark" column="remark"/>
        <property name="paidByBill" column="paid_by_bill"/>
        <property name="quotationId" column="quotation_id"/>
    </class>
</hibernate-mapping>