<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.sf_soft.vehicle.contract.model.VehicleSaleQuotation" table="vehicle_sale_quotation" schema="dbo">
        <id name="quotationId" column="quotation_id"/>
        <property name="vnoId" column="vno_id"/>
        <property name="vehicleVno" column="vehicle_vno"/>
        <property name="shortNameVno" column="short_name_vno"/>
        <property name="vehicleName" column="vehicle_name"/>
        <property name="vehicleStrain" column="vehicle_strain"/>
        <property name="vehicleColor" column="vehicle_color"/>
        <property name="vehiclePriceTotal" column="vehicle_price_total"/>
        <property name="vehiclePrice" column="vehicle_price"/>
        <property name="deposit" column="deposit"/>
        <property name="discountAmount" column="discount_amount"/>
        <property name="vehicleQuantity" column="vehicle_quantity"/>
        <property name="vnoIdNew" column="vno_id_new"/>
        <property name="vehicleVnoNew" column="vehicle_vno_new"/>
        <property name="vehicleNameNew" column="vehicle_name_new"/>
        <property name="isContainInsuranceCost" column="is_contain_insurance_cost"/>
        <property name="insuranceIncome" column="insurance_income"/>
        <property name="insurancePf" column="insurance_pf"/>
        <property name="presentIncome" column="present_income"/>
        <property name="presentPf" column="present_pf"/>
        <property name="conversionIncome" column="conversion_income"/>
        <property name="conversionPf" column="conversion_pf"/>
        <property name="chargeIncome" column="charge_income"/>
        <property name="chargePf" column="charge_pf"/>
        <property name="customerId" column="customer_id"/>
        <property name="customerNo" column="customer_no"/>
        <property name="customerName" column="customer_name"/>
        <property name="vehicleProfit" column="vehicle_profit"/>
        <property name="largessAmount" column="largess_amount"/>
        <property name="belongToSupplierId" column="belong_to_supplier_id"/>
        <property name="belongToSupplierNo" column="belong_to_supplier_no"/>
        <property name="belongToSupplierName" column="belong_to_supplier_name"/>
        <property name="subjectMatter" column="subject_matter"/>
        <property name="startPoint" column="start_point"/>
        <property name="waysPoint" column="ways_point"/>
        <property name="endPoint" column="end_point"/>
        <property name="vehicleOwnerId" column="vehicle_owner_id"/>
        <property name="vehicleOwnerName" column="vehicle_owner_name"/>
        <property name="visitorNo" column="visitor_no"/>
        <property name="imagesUrls" column="images_urls"/>
        <property name="vehicleComment" column="vehicle_comment"/>
        <property name="contractNo" column="contract_no"/>

        <property name="planFinishTime" type="java.sql.Timestamp">
            <column name="plan_finish_time" length="23" />
        </property>
        <property name="buyType" type="java.lang.Short">
            <column name="buy_type" />
        </property>

        <property name="status" column="status"/>

        <property name="seller" column="seller"/>
        <property name="sellerId" column="seller_id"/>
        <property name="createTime" column="create_time" />
        <property name="departmentId" column="department_id" />
        <property name="linkman" column="linkman" />
    </class>
</hibernate-mapping>