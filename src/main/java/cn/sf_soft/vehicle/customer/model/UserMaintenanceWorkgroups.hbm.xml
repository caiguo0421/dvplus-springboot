<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.sf_soft.vehicle.customer.model.UserMaintenanceWorkgroups" abstract="true">
    	<id name="bmwId">
    		<column name="bmw_id"></column>
    		<generator class="assigned"/>
    	</id>
    	<property name="bmwName">
    		<column name="bmw_name"></column>
    	</property>
    	<property name="principal">
    		<column name="principal"></column>
    	</property>
    </class>
</hibernate-mapping>
