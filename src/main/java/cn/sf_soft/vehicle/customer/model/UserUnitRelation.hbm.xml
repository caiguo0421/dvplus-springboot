<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.sf_soft.vehicle.customer.model.UserUnitRelation" abstract="true">
    	<id name="userId">
    		<column name="user_id"></column>
    		<generator class="assigned"/>
    	</id>
    	<property name="userName">
    		<column name="user_name"></column>
    	</property>
    	<property name="userFullName">
    		<column name="user_full_name"></column>
    	</property>
    	<property name="unidId">
    		<column name="unit_id"></column>
    	</property>
    	<property name="unitRelation">
    		<column name="unit_relation"/>
    	</property>
    </class>
</hibernate-mapping>
