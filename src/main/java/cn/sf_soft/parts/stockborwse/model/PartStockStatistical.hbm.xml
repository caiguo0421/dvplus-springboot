<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.sf_soft.parts.stockborwse.model.PartStockStatistical" abstract="true">
    	<id name="totalCount">
    		<column name="total_count"></column>
    		<generator class="assigned"/>
    	</id>
    	<property name="totalCost">
    		<column name="total_cost"></column>
    	</property>
    </class>
</hibernate-mapping>
