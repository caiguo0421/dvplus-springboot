<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.sf_soft.report.model.VehiclePeriodOfTimeReport" abstract="true">
    	<id name="id">
    		<column name="id"></column>
    		<generator class="assigned"/>
    	</id>
    	<property name="stationName">
    		<column name="station_name"></column>
    	</property>
    	<property name="analyseType">
    		<column name="analyse_type"></column>
    	</property>
    	<property name="analyseResult">
    		<column name="analyse_result"></column>
    	</property>
    	<property name="totalProfit">
    		<column name="total_profit"/>
    	</property>
    	<property name="totalProfitPercent">
    		<column name="total_profit_percent"/>
    	</property>
    </class>
</hibernate-mapping>
