<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.sf_soft.report.model.OfficeAssetReport" abstract="true">
    	<id name="id">
    		<column name="id"></column>
    		<generator class="assigned"/>
    	</id>
    	<property name="stationName">
    		<column name="station_name"></column>
    	</property>
    	<property name="type">
    		<column name="type"></column>
    	</property>
    	<property name="typeMeaning">
    		<column name="type_meaning"></column>
    	</property>
    	<property name="quantity">
    		<column name="quantity"></column>
    	</property>
    	<property name="price">
    		<column name="price"></column>
    	</property>
    	<property name="stationId">
    		<column name="station_id"></column>
    	</property>
    </class>
</hibernate-mapping>
